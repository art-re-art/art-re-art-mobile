{
  "expo": {
    "name": "Art/Re/Art",
    "description": "Companion app for a series of pop up art events in downtown Morganton, NC.",
    "slug": "art-re-art",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/art-re-art/art-re-art-mobile",
